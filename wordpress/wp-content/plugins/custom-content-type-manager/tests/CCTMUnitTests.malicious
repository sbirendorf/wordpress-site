	function test_sppost1() {
		$SP = new SP_Post();	
		$post = $SP->get(21);
		$this->assertTrue($post['post_title'] == 'Page C-1');
		
		$post['post_title'] = 'Page C-1-test';
		$SP->update($post,21);
		$post = $SP->get(21);
		$this->assertTrue($post['post_title'] == 'Page C-1-test');
		$post['post_title'] = 'Page C-1';
		$SP->update($post,21);
	}
	
	
